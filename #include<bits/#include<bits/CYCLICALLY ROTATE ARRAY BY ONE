#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    int data[n];
    for(int i = 0 ; i < n; i++){
        cin>>data[i];
    }
    reverse(data,data+n - 1);
    reverse(data,data+n);
    cout<<"the array after one rotation would be"<<endl;
    for(int i = 0 ; i < n ; i++){
        cout<<data[i]<<" ";
    }
    return 0;
}
